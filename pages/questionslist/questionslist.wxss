/*总体主盒子*/  
.container {  

  width: 100%;  
 
  background-color: #fff;  
  color: #939393;  


}  
.q_list{ width:90%; padding-left:5%; padding-right:5%; border-bottom:1px solid #f4f4f4;}
.q_list .q_list_t{ width:100%; font-size:1.2em; color:#000; height:2.5em;line-height:2.5em;overflow: hidden; text-overflow:ellipsis; white-space: nowrap;}
.q_list_b{ width:100%; height: 2.5em; line-height: 2.5em;  font-size: 1em;  }
.b_left{ width: 50%;display: inline-block;overflow: hidden; text-overflow:ellipsis; white-space: nowrap;}
.b_right{ width: 50%;display: inline-block; text-align: right;overflow: hidden; text-overflow:ellipsis; white-space: nowrap;}
.list_no{ width:90%; padding-left:5%; padding-right:5%; }
.list_no .list_img{ width:70%;height:5em; margin:0 auto;}
.list_no .list_img image{width:100%;}